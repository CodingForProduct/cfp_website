<h1>Students</h1>

<ul>
 <% users.false && users.false.forEach(function(user) { %>
  <li>
    <a href='/users/<%= user.id %>' ><%= user.name %></a>
  </li>
<% }) %>
</ul>

<h1>Mentors</h1>

<ul>
 <% users.true && users.true.forEach(function(user) { %>
  <li>
    <a href='/users/<%= user.id %>' ><%= user.name %></a>
    <% if(user.programming_experience) { %>
      <br><%= user.programming_experience %>
    <% } %>

    <% if(user.mentor_topics) { %>
      <br><%= user.mentor_topics %>
    <% } %>
  </li>
<% }) %>
</ul>
